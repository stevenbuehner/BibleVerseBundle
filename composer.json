{
  "name": "stevenbuehner/bible-verse-bundle",
  "description": "A highly tested library to parse, identify, merge store and work with bibleverses.",
  "type": "library",
  "homepage": "https://github.com/stevenbuehner/BibleVerseBundle",
  "license": "MIT",
  "require": {
    "php": ">7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "phpdocumentor/reflection-docblock": "2.0",
    "symfony/yaml": "~2.1",
    "twig/twig": "^1.35"
  },
  "authors": [
    {
      "name": "Steven Buehner",
      "email": "buehner@me.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "StevenBuehner\\BibleVerseBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StevenBuehner\\BibleVerseBundleTests\\": "Tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
